{
  "name": "@example/minimal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "dev-host": "astro dev --host",
    "start": "astro dev",
    "build": "astro build",
    "format": "yarn prettier -w .",
    "preview": "astro preview"
  },
  "devDependencies": {
    "astro": "^3.2.2",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "sass": "^1.49.11"
  },
  "dependencies": {
    "@astrojs/markdown-remark": "^3.2.0",
    "@astrojs/mdx": "^1.1.2",
    "@astrojs/sitemap": "^3.0.0",
    "@astrojs/vue": "^3.0.0",
    "astro-icon": "^0.8.1",
    "cookieconsent": "^3.1.1",
    "luxon": "^2.3.1",
    "vue": "^3.2.26"
  },
  "prettier": {
    "astroAllowShorthand": true,
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ],
    "pluginSearchDirs": [
      "."
    ],
    "plugins": [
      "prettier-plugin-astro"
    ]
  }
}
